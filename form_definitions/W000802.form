<form action="http://www.whitehouse.senate.gov/contact/" method="post" id="contact-form" onsubmit="return formCheck();">
<h3>
<fieldset>
<legend>
<ol class="col two">
<li class="required">
<label for="contact_fname">
<abbr title="Your first name is a required field">
<input type="text" name="fname" id="contact_fname" required="true" placeholder="First Name" value="First Name" onclick="this.value=''" >
<li class="required">
<label for="contact_lname">
<abbr title="Your last name is a required field">
<input type="text" name="lname" id="contact_lname"required="true"  placeholder="Last Name" value="Last Name" onclick="this.value=''">
<ol class="col one">
<li class="required">
<label for="message">
<textarea name="message" id="message">
<input class="button" type="submit" id="submit" value="Continue">
