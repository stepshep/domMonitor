<form method="post" action="https://forms.house.gov/htbin/formproc_za/esty/webforms/issue_subscribe_parm.txt&form=/esty/webforms/issue_subscribe_verify.html">
<fieldset>
<legend>
<label for="prefix" aria-invalid="false">
<input id="prefix" maxlength="100" name="prefix" placeholder="Prefix" type="text" >
<label for="required-first" aria-invalid="false">
<span style="color:#FF0000">
<input class="validate[required] is-required" id="required-first" maxlength="100" name="required-first" placeholder="First Name" type="text" >
<label for="middle">
<input id="middle" maxlength="100" name="middle" placeholder="Middle Initial" type="text" >
<label for="required-last" aria-invalid="false">
<span style="color:#FF0000">
<input class="validate[required] is-required" id="required-last" maxlength="100" name="required-last" placeholder="Last Name" type="text" >
<label for="suffix">
<input id="suffix" maxlength="100" name="suffix" placeholder="Suffix" type="text" >
<label for="required-address" aria-invalid="false">
<span style="color:#FF0000">
<input class="validate[required] is-required" id="required-address" maxlength="100" name="required-address" placeholder="Street Address" type="text" >
<label for="address2">
<input id="address2" maxlength="100" name="address2" placeholder="Street Address Continued" type="text" >
<label for="required-city" aria-invalid="false">
<span style="color:#FF0000">
<input class="validate[required] is-required" id="required-city" maxlength="100" name="required-city" placeholder="City" type="text" >
<label for="required-state" aria-invalid="false">
<span style="color:#FF0000">
<input id="required-state" maxlength="2" name="required-state" value >
<label for="required-zip5" aria-invalid="false">
<span style="color:#FF0000">
<input value id="required-zip5" maxlength="5" name="required-zip5" >
<label for="zip4" aria-invalid="false">
<input value id="zip4" maxlength="5" name="zip4" >
<label for="required-valid-email" aria-invalid="false">
<span style="color:#FF0000">
<input class="validate[required,custom[email]] is-required" id="required-valid-email" maxlength="100" name="required-valid-email" placeholder="Email" type="text" >
<fieldset>
<legend>
<label for="required-phone">
<input id="phone" maxlength="80" name="phone" placeholder="Telephone Number" type="text" >
<input checked="checked" id="speech_0" name="speech" type="radio" value="Voice" >
<label class="is-inline" for="speech_0">
<input id="speech_1" name="speech" type="radio" value="VP" >
<label class="is-inline" for="speech_1">
<input id="speech_2" name="speech" type="radio" value="TTD" >
<label class="is-inline" for="speech_2">
<fieldset>
<legend>
<label for="required-issue">
<select id="issue" name="issue" size="1">
<option selected="selected" value>
<option value="AGR">
<option value="AN">
<option value="ART">
<option value="BANK">
<option value="BUD">
<option value="CFR">
<option value="CONG">
<option value="TOUR">
<option value="DEF">
<option value="ECON">
<option value="EDU">
<option value="ENE">
<option value="ENV">
<option value="FED">
<option value="FR">
<option value="GOV">
<option value="GUN">
<option value="HEA">
<option value="HOM">
<option value="HUD">
<option value="IMM">
<option value="INT">
<option value="JUD">
<option value="LAB">
<option value="LGBT">
<option value="NRN">
<option value="POST">
<option value="SCI">
<option value="SOC">
<option value="SENIORS">
<option value="SSEC">
<option value="SOC.SERVICES">
<option value="TAX">
<option value="TEL">
<option value="TRA">
<option value="TRADE">
<option value="VET">
<option value="WOMEN">
<option value="WEL">
<option value="OTHER">
<label for="subject">
<input id="subject" maxlength="100" name="subject" placeholder="Message Subject" type="text" >
<label for="required-message" aria-invalid="false">
<span style="color:#FF0000">
<textarea class="validate[required] is-required" cols="68" id="required-message" name="required-message" placeholder="Text of Message" rows="5">
<input checked="checked" id="subscribe" name="subscribe" type="checkbox" value="Email.Optin" >
<label class="is-inline" for="subscribe">
<label class="is-inline" for="required-response" aria-invalid="false">
<span style="color:#FF0000">
<select class="validate[required] is-required" id="required-response" name="required-response" size="1">
<option selected="selected" value>
<option value="Y">
<option value="N">
<input class="submit" id="submit" name="submit" role="button" type="submit" value="Send Email" >
