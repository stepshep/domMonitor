<form method="post" action="http://www.senate.state.tx.us/bin/NLhandler.php" class="memForm">
<h3 class="formtitle">
<p>
<b>
<i>
<em>
<LABEL for="SENDER">
<input type="text" name="sender" id="SENDER" size="35" maxlength="40" >
<LABEL for="NAME">
<input type="text" name="name" id="NAME" size="30" maxlength="30" >
<LABEL for="PHONE">
<input type="text" name="phone" id="PHONE" size="14" maxlength="15" >
<LABEL for="STREET">
<input type="text" name="address" id="STREET" size="40" maxlength="44" >
<LABEL for="CITY">
<input type="text" name="city" id="CITY" size="20" maxlength="25" >
<LABEL for="STATE">
<input type="text" name="state" id="STATE" size="15" maxlength="15" >
<LABEL for="ZIPCODE">
<input type="text" name="zip" id="ZIPCODE" size="10" maxlength="12" >
<LABEL for="MESSAGE">
<input type="radio" name="msg" id="MESSAGE" value="Subscription Request: Please ADD me to the mailing list for Senator Zaffirini's Reports to the Families of SD21" checked="checked" >
<input type="radio" name="msg" id="MESSAGE" value="UNSUBSCRIBE Request: Please REMOVE me from the mailing list for Senator Zaffirini's Reports to the Families of SD21." >
<input type="submit" value="Submit" >
<input type="reset" value="Reset" >
