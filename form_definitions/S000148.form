<label for="home_phone_number">
<abbr title="Your Phone number is a required field">
<input class="form-control" type="text" name="home_phone_number" id="home_phone_number" aria-required="true" placeholder="Required" required>
<label for="email_address">
<abbr title="Your e-mail address is a required field">
<input class="form-control" type="text" name="email_address" aria-required="true" id="email" placeholder="Required" required>
<label for="verify_email">
<abbr title="Please confirm your e-mail address">
<input class="form-control" type="text" name="verify_email" id="verify_email" aria-required="true" placeholder="Required" required>
<fieldset>
<legend>
<label class="control-label" for="subject">
<abbr title="Please enter a message topic for your message">
<select class="form-control" id="subject" name="subject">
