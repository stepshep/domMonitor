<form id="contactForm" method="post" action="/contact/email-chuck" enctype="application/x-www-form-urlencoded">
<fieldset>
<label class="control-label" for="actions">
<abbr title="Please select an option">
<select class="form-control" id="actions" name="actions" onchange="javascript:getHelp(this.value);">
<option label="- - Select an option - -" value="select" selected="selected">
<option label="Share your opinion or comments on bills or other issues" value="opinion">
<option label="Request a flag to be flown over the Capitol" value="flag">
<option label="Request help with a federal agency" value="federal_agency">
<fieldset>
<legend>
<label class="control-label" for="salutation">
<abbr title="required">
<select class="form-control" id="salutation" name="salutation" required>
<option label="Mr." value="Mr.">
<option label="Mrs." value="Mrs.">
<option label="Ms." value="Ms.">
<option label="Dr." value="Dr.">
<label for="fname">
<abbr title="required">
<input class="form-control" type="text" name="fname" id="fname" aria-required="true" placeholder="Required" required>
<label for="lname">
<abbr title="required">
<input class="form-control" type="text" name="lname" id="lname" aria-required="true" placeholder="Required" required>
<fieldset>
