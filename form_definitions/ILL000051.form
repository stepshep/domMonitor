<form action=/index.php/contact-us method=post name=emailForm id=emailForm class=form-validate>
<fieldset>
<legend>
<label class=label-top for=contact_name>
<input type=text name=name id=contact_name size=30 class=inputbox value= >
<label class=label-top for=contact_email>
<input type=text id=contact_email name=email size=30 value= class=inputbox required validate-email maxlength=100 >
<label class=label-top for=contact_subject>
<input type=text name=subject id=contact_subject size=30 class=inputbox value= >
<label class=label-top for=contact_text>
<textarea cols=50 rows=10 name=text id=contact_text class=inputbox required>
<input type=checkbox name=email_copy id=contact_email_copy value=1  >
<label for=contact_email_copy>
<button class=button validate type=submit>
