<form class="form-validate" action="/index.php/contact-us" method="post" name="emailForm" id="emailForm">
<fieldset>
<legend>
<label class="label-top" for="contact_name">
<input class="inputbox" type="text" name="name" id="contact_name" size="30" value >
<label class="label-top" for="contact_email">
<input class="inputbox required validate-email" type="text" id="contact_email" name="email" size="30" value maxlength="100" >
<label class="label-top" for="contact_subject">
<input class="inputbox" type="text" name="subject" id="contact_subject" size="30" value >
<label class="label-top" for="contact_text">
<textarea class="inputbox required" cols="50" rows="10" name="text" id="contact_text">
<input type="checkbox" name="email_copy" id="contact_email_copy" value="1" >
<label for="contact_email_copy">
<button class="button validate" type="submit">
